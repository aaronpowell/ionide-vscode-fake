{
	"name": "ionide-fake",
	"version": "0.0.1",
	"publisher": "Ionide",
	"engines": {
		"vscode": "^0.10.0"
	},
	"main": "./fake",
	"contributes": {
		"commands": [{
			"command": "fake.fakeBuild",
			"title": "FAKE: Build"
		},
		{
			"command": "fake.cancelBuild",
			"title": "FAKE: Cancel"
		},
		{
			"command": "fake.buildDefault",
			"title": "FAKE: Build Default"
		}],
		"outputChannels": [
			"FAKE"
		],
		"keybindings": [{
        "command": "fake.buildDefault",
        "key": "ctrl+f5",
        "mac": "cmd+f5"
    }]
	},
	"activationEvents": [
    "onCommand:fake.fakeBuild",
		"onCommand:fake.cancelBuild",
		"onCommand:fake.buildDefault"
	],
	"devDependencies": {
		"vscode": "*"
	},
	"dependencies": {
		"path": "*",
    "toml": "*"
	}
}
